    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/def/digitaltwin#aggregate -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#aggregate">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#AggregatedDigitalTwin"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinInstance"/>
        <rdfs:comment xml:lang="en">Property that describes de action of aggregation between two different entities or dt:DigitalTwin.</rdfs:comment>
        <rdfs:label>aggregate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasComponent -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasComponent">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#PhysicalEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#Component"/>
        <rdfs:comment xml:lang="en">Property that describes the different components that the dt:PhysicalEntitty contains.</rdfs:comment>
        <rdfs:label>has component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasConnection -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasConnection">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinConnection"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#Connection"/>
        <rdfs:comment xml:lang="en">Property that describes the different connections that the dt:DigitalTwinConnections contains.</rdfs:comment>
        <rdfs:label>has connection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasConnectionPoint -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasConnectionPoint">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#Connection"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#ConnectionPoint"/>
        <rdfs:label>has connection point</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasData -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasData">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinData"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <rdfs:comment xml:lang="en">Property that describes the different data that the dt:DigitalTwinData contains.</rdfs:comment>
        <rdfs:label>has data</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasDigitalTwinConnection -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasDigitalTwinConnection">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwin"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinConnection"/>
        <rdfs:comment xml:lang="en">Property that describes the different Digital Twin Connections that the digital twin contains.</rdfs:comment>
        <rdfs:label>has digital twin connection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasDigitalTwinData -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasDigitalTwinData">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwin"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinData"/>
        <rdfs:comment xml:lang="en">Property that describes the different Digital Twin Data that the digital twin contains.</rdfs:comment>
        <rdfs:label>has digital twin data</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasDigitalTwinService -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasDigitalTwinService">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwin"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinService"/>
        <rdfs:label>has digital twin service</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasFormat -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasFormat">
        <rdfs:comment xml:lang="en">Property that describes the different extension formats that the models or data can be found.</rdfs:comment>
        <rdfs:label>has format</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasFormats -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasFormats">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#Model"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#Format"/>
        <rdfs:label>has formats</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasInteractionAffordance -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasInteractionAffordance">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinService"/>
        <rdfs:range rdf:resource="https://www.w3.org/2019/wot/td#InteractionAffordance"/>
        <rdfs:label>has interaction affordance</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasModel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasModel">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#VirtualEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#Model"/>
        <rdfs:comment xml:lang="en">Property that describes the different models that the dt:VirtualEntitty contains.</rdfs:comment>
        <rdfs:label>has model</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasPhysicalEntity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasPhysicalEntity">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwin"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#PhysicalEntity"/>
        <rdfs:comment xml:lang="en">Property that describes the different Physical Entities that the digital twin contains.</rdfs:comment>
        <rdfs:label>has physical entity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasVirtualEntity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/digitaltwin#hasVirtualEntity">
        <rdfs:domain rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwin"/>
        <rdfs:range rdf:resource="https://w3id.org/def/digitaltwin#VirtualEntity"/>
        <rdfs:comment xml:lang="en">Property that describes the different Virtual Entities that the digital twin contains.</rdfs:comment>
        <rdfs:label>has virtual entity</rdfs:label>
    </owl:ObjectProperty>
    


