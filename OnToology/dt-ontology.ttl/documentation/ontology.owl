    <!-- http://purl.org/dc/terms/description -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/description">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/identifier -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/identifier">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:label>identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/title">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/digitaltwin#hasExtension -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/digitaltwin#hasExtension">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Extension of the model or data with which can be found.</rdfs:comment>
        <rdfs:label>has extension</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/digitaltwin#isInternal -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/digitaltwin#isInternal">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Boolean that indicates if the concept is inside or outside the digital twin.</rdfs:comment>
        <rdfs:label>is internal</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/digitaltwin#isProvider -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/digitaltwin#isProvider">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Boolean that indicates if the member of the connection is the one that provides the information or a specific information, or not.</rdfs:comment>
        <rdfs:label>is provider</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://www.w3.org/2019/wot/hypermedia#hasTarget -->


    <owl:DatatypeProperty rdf:about="https://www.w3.org/2019/wot/hypermedia#hasTarget">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:label>has target</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://www.w3.org/2019/wot/td#hasForm -->


    <owl:DatatypeProperty rdf:about="https://www.w3.org/2019/wot/td#hasForm">
        <rdfs:label>has form</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://www.w3.org/2019/wot/td#hasUriTemplateSchema -->


    <owl:DatatypeProperty rdf:about="https://www.w3.org/2019/wot/td#hasUriTemplateSchema">
        <rdfs:label>has uri template schema</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/ns/dcat#Resource -->


    <owl:Class rdf:about="http://www.w3.org/ns/dcat#Resource">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#ConnectionPoint"/>
        <rdfs:label>Resource</rdfs:label>
    </owl:Class>
    


    <!-- https://saref.etsi.org/core/Actuator -->


    <owl:Class rdf:about="https://saref.etsi.org/core/Actuator">
        <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core/Device"/>
        <rdfs:label>Actuator</rdfs:label>
    </owl:Class>
    


    <!-- https://saref.etsi.org/core/Device -->


    <owl:Class rdf:about="https://saref.etsi.org/core/Device">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#Component"/>
        <rdfs:label>Device</rdfs:label>
    </owl:Class>
    


    <!-- https://saref.etsi.org/core/Sensor -->


    <owl:Class rdf:about="https://saref.etsi.org/core/Sensor">
        <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core/Device"/>
        <rdfs:label>Sensor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#AggregatedDigitalTwin -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#AggregatedDigitalTwin">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwin"/>
        <rdfs:comment xml:lang="en">Describes the aggregation of all the DTIs. Unlike the DTI, the DTA may not be an independent data structure. It may be a computing construct that has access to all DTIs and queries them either ad-hoc or proactively.</rdfs:comment>
        <rdfs:label>Aggregated Digital Twin</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#BehavioralModel -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#BehavioralModel">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#Model"/>
        <rdfs:comment xml:lang="en">Describes the behaviors and response mechanisms of the entity (e.g. state transition, performance and disturbances)</rdfs:comment>
        <rdfs:label>Behavioral Model</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#Component -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#Component">
        <rdfs:comment xml:lang="en">Subsystem, object or sensory device registered in the Physical Entity dimension.</rdfs:comment>
        <rdfs:label>Component</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#Connection -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#Connection">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/digitaltwin#hasConnectionPoint"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Existing connection between different dt:DigitalTwinDimension.</rdfs:comment>
        <rdfs:label>Connection</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#ConnectionPoint -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#ConnectionPoint">
        <rdfs:label>Connection Point</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#DigitalTwin -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#DigitalTwin">
        <rdfs:subClassOf rdf:resource="https://www.w3.org/2019/wot/td#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/digitaltwin#hasDigitalTwinConnection"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/digitaltwin#hasDigitalTwinData"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/digitaltwin#hasDigitalTwinService"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/digitaltwin#hasPhysicalEntity"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/digitaltwin#hasVirtualEntity"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A realistic representation of all product life cycle components using physical, virtual and interaction data.</rdfs:comment>
        <rdfs:label>Digital Twin</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#DigitalTwinConnection -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#DigitalTwinConnection">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinDimension"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/digitaltwin#hasConnection"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This dimension of the digital twin contains all the existing connections that are between the different dimensions of the digital twins and the information used.</rdfs:comment>
        <rdfs:label>Digital Twin Connection</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#DigitalTwinData -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#DigitalTwinData">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinDimension"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/digitaltwin#hasData"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This dimension of the digital twin contains all the data used by the digital twin.</rdfs:comment>
        <rdfs:label>Digital Twin Data</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#DigitalTwinDimension -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#DigitalTwinDimension">
        <rdfs:comment xml:lang="en">Dimensions of which the digital twin is composed.</rdfs:comment>
        <rdfs:label>Digital Twin Dimension</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#DigitalTwinInstance -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#DigitalTwinInstance">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwin"/>
        <rdfs:comment xml:lang="en">Describes a specific corresponding physical product that an individual Digital Twin remains linked to throughout the life of that physical product.</rdfs:comment>
        <rdfs:label>Digital Twin Instance</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#DigitalTwinService -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#DigitalTwinService">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#DigitalTwinDimension"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/digitaltwin#hasInteractionAffordance"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This dimension of the digital twin contains all the services that are used to perform all the processes of the digital twin.</rdfs:comment>
        <rdfs:label>Digital Twin Service</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#Format -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#Format">
        <rdfs:comment xml:lang="en">Extension format in which the model or data is available.</rdfs:comment>
        <rdfs:label>Format</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#GeometricModel -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#GeometricModel">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#Model"/>
        <rdfs:comment xml:lang="en">Describes all the geometric parameters of the physical entity (e.g. shapes, sizes and assembly relations).</rdfs:comment>
        <rdfs:label>Geometric Model</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/digitaltwin#MainComponent -->


    <owl:Class rdf:about="https://w3id.org/def/digitaltwin#MainComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/digitaltwin#Component"/>
        <rdfs:comment xml:lang="en">Main component of the Physical Entity dimension in the case of multiple.</rdfs:comment>
        <rdfs:label>Main Component</rdfs:label>
    </owl:Class>
    


