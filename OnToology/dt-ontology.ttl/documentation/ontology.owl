<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/def/digitaltwin#"
     xml:base="https://w3id.org/def/digitaltwin"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:mod="https://w3id.org/mod#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/">
    <owl:Ontology rdf:about="https://w3id.org/def/digitaltwin#">
        <dc:contributor>Andrea Cimmino</dc:contributor>
        <dc:contributor>María Poveda Villalón</dc:contributor>
        <dc:contributor>Raúl García Castro</dc:contributor>
        <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-01-18</dc:created>
        <dc:creator>Salvador González Gerpe</dc:creator>
        <dc:description xml:lang="en">Digital Twin ontology used to define Digital Twins and Semantic Digital Twins by dimensions.</dc:description>
        <dc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <dc:title xml:lang="en">The Digital Twin ontology</dc:title>
        <vann:preferredNamespacePrefix>dt</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:resource="https://w3id.org/def/digitaltwin#"/>
        <owl:versionInfo>0.1.0</owl:versionInfo>
        <mod:createdWith rdf:resource="https://chowlk.linkeddata.es/"/>
    </owl:Ontology>
    


